<odoo>
    <data>

        <record model="ir.actions.act_window" id="stock_list_action">
            <field name="name">Stock </field>
            <field name="res_model">gctjara.stock</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="context"
                eval="{'search_default_filter': 1,'search_default_by_responsible': 1}" />
        </record>
 <record model="ir.ui.view" id="stock_form_view">
            <field name="name">stock.form</field>
            <field name="model">gctjara.stock</field>
            <field name="arch" type="xml">
                <form string="Produits Form">

                    <sheet>
                        <group>
                            <field name="numero" />
                            <field name="mouvement_id" />
                            <field name="depot_id" />
                            <field name="embalageproduit_id" />
                        </group>
                    </sheet>
                     <notebook>
                        
                            <page string="Achats" class="oe_page_Title">
                                <group>
                                    <field name="lignefactureachat_id">
                                        <tree>
                                           <field name="name" String="Stock"/>
                                        </tree>
                                    </field>
                                </group>
                            </page>
                            <page string="Ventes" class="oe_page_Title">
                                <group>
                                    <field name="lignefacturevente_id">
                                        <tree>
                                           <field name="name" String="Emballage"/>
                                           <field name="quantite" String="QuantitÃ©"/>
                                        </tree>
                                    </field>
                                </group>
                            </page>
                        </notebook>
                </form>
            </field>
        </record>
        
        <record model="ir.ui.view" id="stock_tree_view">
            <field name="name">stock.tree</field>
            <field name="model">gctjara.stock</field>
            <field name="arch" type="xml">
                <tree string="Stock Form" >
                     <field name="numero" />
                     <field name="mouvement_id" />
                     <field name="depot_id" />
                     <field name="embalageproduit_id" />
                </tree>
            </field>
        </record>
    </data>
</odoo>